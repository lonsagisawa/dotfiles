!TomorrowNight

*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #aeafad

urxvt.font: xft:monospace-12
URxvt.preeditType: root
urxvt.scrollBar: false
urxvt.termName: xterm
urxvt.saveLines: 65535

! Black / Grey
*.color0:       #000000
*.color8:       #666666

! Red / Bright Red
*.color1:       #cc6666
*.color9:       #FF3334

! Green + Bright Green
*.color2:       #b5bd68
*.color10:      #9ec400

! Yellow (Orange) + Bright Yellow (Yellow)
*.color3:       #de935f
*.color11:      #f0c674

! Blue + Bright Blue
*.color4:       #81a2be
*.color12:      #81a2be

! Magenta (Purple) + Bright Magenta
*.color5:       #b294bb
*.color13:      #b777e0

! Cyan (Aqua) + Bright Cyan
*.color6:       #8abeb7
*.color14:      #54ced6

! Light Grey (Selection) + White (Current Line)
*.color7:       #373b41
*.color15:      #282a2e
