#!/bin/bash

WIRELESS_INTERFACE=wlp3s0

case "$(cat /sys/class/net/enp0s25/operstate 2>/dev/null)" in
	up) printf "  " ;;
esac	

case "$(cat /sys/class/net/wlp3s0/operstate 2>/dev/null)" in
	up) printf " $(iw dev wlp3s0 info | grep "ssid" | sed s/ssid//g | tr -d \[:space:\]) " ;;
esac

# vim: ft=sh
